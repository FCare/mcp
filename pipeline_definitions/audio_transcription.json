{
  "name": "Pipeline Audio Transcription WebSocket",
  "description": "Pipeline audio streaming avec WebSocket et transcription Kyutai STT",
  "category": "audio_ai",
  "version": "1.0.0",
  "step_instances": [
    {
      "instance_id": "websocket_server",
      "step_definition": "websocket_server",
      "config_overrides": {
        "port": 8766,
        "mode": "audio_to_text",
        "audio_format": "pcm16",
        "sample_rate": 24000
      }
    },
    {
      "instance_id": "asr_step",
      "step_definition": "Kyutai ASR Step",
      "config_overrides": {}
    }
  ],
  "connections": [
    {
      "from": "websocket_server",
      "to": "asr_step"
    },
    {
      "from": "asr_step",
      "to": "websocket_server"
    }
  ]
}